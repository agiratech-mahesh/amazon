pipeline { 
    agent any 
        stages { 
            stage ('checkout') { 
                steps { 
                     checkout scm
                }  
            }
            stage ('build') {
                steps {
                echo"build is complete"
                }
                 }
            stage ('test') {
            steps {
            echo"test is complete"
            }
            }
            stage ('deploy') {
            steps {
            sh " cd /home/agira/amazon/index.html && cp . -r /var/www/html/amazon.com/public_html/index.html "
            }
            }
        }
    }
