    agent any 
        stages { 
            stage ('checkout') { 
                steps { 
                     checkout scm
                }  
            }
            stage ('build') {
                steps {
                echo"build is complete"
                }
                 }
            stage ('test') {
            steps {
            echo"test is complete"
            }
            }
            stage ('deploy') {
            steps {
            sh " cp /home/agira/amazon /var/www/amazon.com/public_html/"
            }
            }
        }
    }
